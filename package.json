{
  "name": "multipurpose-api",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:webpack": "rm -rf lib && webpack",
    "build:move": "npm run build:webpack && rm -rf asset*/lib && mv lib asset*/",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "remove-tar": "rm .deploy/multipurpose-api.zip | exit 0",
    "compress": "npm run remove-tar && npm run build:webpack && bestzip .deploy/multipurpose-api.zip ./lib",
    "copy-data": "cp -r ./src/data asset*/lib",
    "copy-node-modules": "cp -r ./node_modules asset*/lib"
  },
  "devDependencies": {
    "@aws-cdk/aws-iam": "^1.79.0",
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/aws-lambda": "^8.10.68",
    "@types/node": "^14.14.16",
    "babel-loader": "^8.2.2",
    "bestzip": "^2.1.7",
    "typescript": "^3.7.5",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "aws-sdk": "^2.817.0",
    "core-js": "^3.8.3",
    "regenerator-runtime": "^0.13.7",
    "util": "^0.12.3"
  }
}
